<p id="notice"><%= notice %></p>
div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Date Ideas
                    <small>for <%=@current_user.first_name%></small>
                </h1>
            </div>
        </div>







    <div>
          <input type="text" class="form-control js-zip" placeholder="Search">
        </div>
    <button type="submit" class="btn btn-default js-search">Submit</button>
<ul class = "js-date-list">
</ul>


<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @date_ideas.each do |date_idea| %>
      <tr>
        <td><%=date_idea.name%></td>
        <td><%= link_to 'Show', date_idea %></td>
        <td><%= link_to 'Edit', edit_date_idea_path(date_idea) %></td>
        <td><%= link_to 'Destroy', date_idea, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Date Idea', new_date_idea_path %>
